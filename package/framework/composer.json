{
    "name": "thinkphp/framework",
    "description": "The ThinkPHP Framework",
    "license": "Apache-2.0",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "filp/whoops": "^2.13",
        "nesbot/carbon": "^2.50"
    },
    "autoload": {
        "psr-4": {
            "Think\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
