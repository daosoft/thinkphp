{
    "name": "daosoft/thinkphp",
    "type": "project",
    "description": "The ThinkPHP Framework",
    "keywords": ["framework","thinkphp","daosoft"],
    "homepage": "http://thinkphp.cn/",
    "license": "Apache-2.0",
    "require": {
        "php": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Think\\": "bootstrap/src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
