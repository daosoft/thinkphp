{
    "name": "daosoft/thinkphp",
    "type": "project",
    "description": "The ThinkPHP Framework",
    "keywords": [
        "framework",
        "thinkphp",
        "daosoft"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": "^7.3|^8.0",
        "thinkphp/framework": "4.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "local": {
            "type": "path",
            "url": "package/framework"
        }
    }
}
